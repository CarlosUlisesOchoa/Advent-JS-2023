{
  "extends": [
    "./node_modules/ts-standard/eslintrc.json",
    "prettier"
  ],
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          [
            "@/",
            "src/"
          ]
        ],
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    }
  }
}